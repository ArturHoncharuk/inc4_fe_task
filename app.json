{
  "expo": {
    "name": "HashWatcher",
    "slug": "inc4-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/app/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/shared/assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/app/adaptive-icon.png",
        "backgroundColor": "#faffff"
      }
    },
    "web": {
      "favicon": "./src/shared/assets/images/app/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
